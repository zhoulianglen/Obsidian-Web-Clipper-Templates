{
	"schemaVersion": "0.1.0",
	"name": "YouTube",
	"behavior": "create",
	"noteContentFormat": "![{{schema:name}}]({{url}})",
	"properties": [
		{
			"name": "分类",
			"value": "[[视频]]",
			"type": "text"
		},
		{
			"name": "标题",
			"value": "{{schema:name}}",
			"type": "text"
		},
		{
			"name": "作者",
			"value": "{{schema:author|wikilink}}",
			"type": "multitext"
		},
		{
			"name": "类型",
			"value": "",
			"type": "text"
		},
		{
			"name": "url",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "发布时间",
			"value": "{{schema:uploadDate|date:YYYY-MM-DD}}",
			"type": "date"
		},
		{
			"name": "创建时间",
			"value": "{{date}}",
			"type": "date"
		},
		{
			"name": "tags",
			"value": "videos",
			"type": "multitext"
		},
		{
			"name": "img",
			"value": "{{schema:thumbnailUrl|slice:0}}",
			"type": "text"
		}
	],
	"triggers": [
		"https://www.youtube.com/watch?v="
	],
	"noteNameFormat": "{{schema:author}} – {{schema:name}}",
	"path": "Clippings"
}