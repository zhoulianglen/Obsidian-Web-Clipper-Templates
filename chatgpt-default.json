		{
			"name": "title",
			"value": "{{title}}",
			"type": "text"
		},
		{
			"name": "url",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "作者",
			"value": "ChatGPT",
			"type": "multitext"
		},
		{
